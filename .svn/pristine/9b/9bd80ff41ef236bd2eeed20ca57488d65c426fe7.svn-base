@charset "utf-8";
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,blockquote,th,td{margin:0;padding:0;}
a:link,a:visited{text-decoration: none;}
a img{border: 0;}
ul li{list-style: none;}
/*兼容性解决 安卓原生浏览器在加了bg后才渲染为block*/
h1,h2,h4,h5,p{background:rgba(255,255,255,0.01)}
/* 清理浮动 */
.f-clear:after {visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;}
.f-clear {zoom:1;}/* for IE6 IE7 */
input{outline: none;}
.bott-box{padding-bottom: 110px;}
.clear{clear: both}
body{margin: 0;padding: 0;font-family:"微软雅黑";background: #F1F5F8}
.wrapper{width: 640px; background: #F1F5F8; min-height: 1000px; margin:0 auto; overflow: hidden;border-left: 1px solid #d5d5d5;border-right: 1px solid #d5d5d5}

.wx3 .banner{width: 640px;height: 260px;}
.wx3 .title {padding: 20px 0; background: #fff; border-top:1px solid #e5e5e5;border-bottom:1px solid #e5e5e5 }
.wx3 .title p.name{font-size: 26px;color: #333;height: 30px;line-height: 30px;padding-left: 20px;border-left: 6px solid #6cd1e5;}
.wx3 .dp-box{padding: 20px;background: #fff; border-bottom: 1px solid #e5e5e5; margin-bottom: 20px;}
.wx3 .dp-box ul{overflow: hidden;}
.wx3 .dp-box ul li{width: 260px;height: 150px; padding: 20px; float: left;margin-left: -1px;border-right: 1px solid #e5e5e5;border-bottom: 1px solid #e5e5e5}
.wx3 .dp-box ul li.t2{border-right: none}
.wx3 .dp-box ul li.t3{border-bottom: none}
.wx3 .dp-box ul li.t4{border-bottom: none;border-right: none}

.wx3 .dp-box ul li.t1 .p1{font-size: 24px; height: 30px;line-height: 30px; font-weight: bold; color: #ffa200;text-align: center;}
.wx3 .dp-box ul li.t2 .p1{font-size: 24px; height: 30px;line-height: 30px; font-weight: bold; color: #72b8e6;text-align: center;}
.wx3 .dp-box ul li.t3 .p1{font-size: 24px; height: 30px;line-height: 30px; font-weight: bold; color: #4ecd91;text-align: center;}
.wx3 .dp-box ul li.t4 .p1{font-size: 24px; height: 30px;line-height: 30px; font-weight: bold; color: #ff7676;text-align: center;}
.wx3 .dp-box ul li .p1 img{vertical-align: bottom;margin-right: 10px}
.wx3 .dp-box ul li .p2{font-size: 18px;color: #666;height: 30px;line-height: 30px;text-align: center;padding: 10px 0}
.wx3 .dp-box ul li .p3{font-size: 24px;color: #ec2f2f;text-align: center;height: 30px;line-height: 30px;padding-bottom: 10px}
.wx3 .dp-box ul li .p4{font-size: 18px;color: #bbbbbb;text-align: center;}

.set-box{background: #fff;min-height: 100px;border-bottom: 1px solid #e5e5e5; margin-bottom: 20px;}
.set-box ul{overflow: hidden;}
.set-box ul li.list1{margin-left: 30px;height: 80px;padding: 25px 0;border-bottom: 1px solid #e5e5e5;background: url('../images/index/pad_arr.png') no-repeat center right;cursor: pointer;}
.set-box ul li.list2{margin-left: 30px;height: 80px;padding: 25px 0;border-bottom: 1px solid #e5e5e5;background: url('../images/index/pad_arr.png') no-repeat center right;cursor: pointer;}
.set-box ul li:last-child{border-bottom: none}
.set-box ul li.list1 .box1{width: 300px;padding-left: 70px;float: left;background: url('../images/index/list_ico1.png') no-repeat center left;}
.set-box ul li.list2 .box1{width: 300px;padding-left: 70px;float: left;background: url('../images/index/list_ico2.png') no-repeat center left;}
.set-box ul li .box2{width: 180px;float: left;text-align: right;}
.set-box ul li.list1 .box1 .name{font-size: 24px;color: #72b8e9;height: 40px;line-height: 40px}
.set-box ul li.list2 .box1 .name{font-size: 24px;color: #ffb400;height: 40px;line-height: 40px}
.set-box ul li .box1 .text{font-size: 18px;color: #666;height: 30px;line-height: 30px}
.set-box ul li .box2 .price{font-size: 24px;color: #ec2f2f;height: 40px;line-height: 40px}
.set-box ul li .box2 .text{font-size: 18px;color: #bbbbbb;height: 30px;line-height: 30px}

.bot-nav{width: 640px;height: 70px; padding: 15px 0; border-top: 1px solid #e5e5e5;background: #fff;display: none;}
.bot-nav ul{overflow: hidden;}
.bot-nav ul li{width: 33.3%;float: left;}
.bot-nav ul li a{display: block;text-align: center;font-size: 18px;height: 30px;line-height: 30px;padding-top:40px;}
.bot-nav ul li a.n1{background: url('../images/index/nav_ico1.png') no-repeat center top; color: #646464;}
.bot-nav ul li a.n1:hover{background: url('../images/index/nav_icoh1.png') no-repeat center top; color:#4accfb}
.bot-nav ul li a.n2{background: url('../images/index/nav_ico2.png') no-repeat center top; color: #646464; }
.bot-nav ul li a.n2:hover{background: url('../images/index/nav_icoh2.png') no-repeat center top; color:#4accfb}
.bot-nav ul li a.n3{background: url('../images/index/nav_ico3.png') no-repeat center top; color: #646464;}
.bot-nav ul li a.n3:hover{background: url('../images/index/nav_icoh3.png') no-repeat center top; color:#4accfb}
.wx-navxf {position: fixed;bottom: 0px;z-index: 9000;display: block;}



/*产品页*/
.pbanner {background: url("../images/product/regbanner.jpg") no-repeat scroll center center #ffffff;height: 370px; }
.banner-box {background: url("../images/product/b_box.png") repeat-x bottom;height: 115px;padding: 255px 20px 0;}
.banner-box .t1{font-size:36px;color: #ffffff;height: 70px;line-height: 70px }
.banner-box .t2{font-size:24px;color: #ffffff; }
.price-info{min-height: 50px;background: #ffffff;border-bottom: 1px solid #e5e5e5;}
.price-info .price-b1{width: 640px;height: 100px;}
.price-info .price-b1 .p1{padding: 0 20px;line-height: 100px;  font-size: 22px;color: #ff4444}
.price-info .price-b1 .p1 span{font-size: 48px;}
.price-info .price-b1 p{float: left;}
.price-info .price-b1 .p2{padding: 0 80px 0 0;text-decoration: line-through; margin-top: 10px; height: 90px;line-height: 90px;font-size: 26px;color: #999999;}

.check-box{margin-top: 20px;}
.check-box .box{min-height: 55px; line-height: 55px; margin-top: -1px; border: 1px solid #e5e5e5;padding: 15px 20px ;cursor: pointer; background: #ffffff url("../images/ico/pad_arr.png") no-repeat center right}
.check-box .box label.type{font-size: 26px; height: 55px;line-height: 55px; width: 150px; display: inline-block; color: #333333;float: left;}
.check-box .box input.cval{font-size: 26px; height: 55px;line-height: 55px;width: 400px;text-align: right;border: none}

.title-box{height: 35px; line-height: 35px; background: #ffffff; border: 1px solid #e5e5e5;margin-top: 20px;padding: 15px 20px }
.title-box p.tit{font-size: 26px; height: 35px;line-height: 35px; color: #333333;padding-left: 15px;border-left: 6px solid #6cd1e5}
.book-flow{padding: 50px 30px;font-size: 24px;color: #666666;background: #ffffff;}
.book-flow .fole-off{padding-left: 40px; min-height: 24px;line-height: 24px; padding-bottom: 40px; background: url("../images/ico/g_ico.png") no-repeat left top}
.book-flow .fole-off:last-child{padding-left: 40px; min-height: 24px;line-height: 24px; padding-bottom: 40px; background: url("../images/ico/g_ico1.png") no-repeat left top}
.book-flow .fole-on{padding-left: 40px; min-height: 24px;line-height: 24px; padding-bottom: 40px; color: #72b8e9; background: url("../images/ico/b_ico.png") no-repeat left top}
.book-flow .fole-on:last-child{padding-left: 40px; min-height: 24px;line-height: 24px; color: #72b8e9; padding-bottom: 40px; background: url("../images/ico/b_ico1.png") no-repeat left top}

.book-flow .flow-time{padding-top: 15px;color: #999}

.pro-con{padding: 10px 30px 30px 40px; background: #ffffff;border-bottom: 1px solid #e5e5e5;font-size: 26px;color: #666666;}
.pro-con ul{overflow:hidden;}
.pro-con ul li{margin: 20px 0; padding-left: 35px; background: url("../images/ico/p_ico1.png") no-repeat left top}
.pro-tag{font-size: 26px; margin: 10px 0; color: #ff5a5a;padding-left: 40px;background: url("../images/ico/p_ico2.png") no-repeat left top}

.que-box{background: #ffffff;border-bottom: 1px solid #e5e5e5;min-height: 200px}
.que-box ul{overflow:hidden;margin-left: 40px;}
.que-box ul li{border-bottom: 1px solid #e5e5e5;min-height: 100px; overflow: hidden; padding: 30px 30px 20px 0}
.que-box ul li p.que{font-size: 24px;color: #666;min-height: 30px;line-height: 30px; padding-left: 40px; background: url("../images/ico/que_ico.png") no-repeat left top}
.que-box ul li p.ans{font-size: 26px;color: #333; padding: 15px 0; min-height: 40px;line-height: 40px; padding-left: 40px; }


/*省份城市*/
.boxname .box-t{height: 70px;line-height: 70px;font-size: 26px;color: #18b4ed;background: #eeeeee;border-bottom: 1px solid #dddddd}
.boxname .box-t .btn1{float: left;width: 420px; margin-left: 110px; cursor: pointer;font-size: 22px;color: #999999;text-align: center;font-weight: 100}
.boxname .box-t .btn2{float: left;width: 110px;text-align: center;cursor: pointer;}
.boxname .box-t .btn2:hover{background: #dddddd}

.area-box {width:100%;position:relative;}
.area-box .tabbtn {height:380px; width: 210px; float: left; background: #ebebeb}
.area-box .tabbtn li{padding: 0}
.area-box .tabbtn li a {display:block;color: #999999; height: 80px;line-height: 80px; width:210px;text-align:center;font-size:30px; color: #333333; cursor:pointer;}
.area-box .tabbtn li.current {color: #4cbdea}
.area-box .tabbtn li.current a {background:#ffffff;color:#4cbdea;font-weight: 800}
.area-box .tabcon { float: left; background: #ffffff; left: 150px; width: 430px; height:380px;overflow-y:auto;}
.area-box .tabcon li p.city{text-align: center;}
.area-box .tabcon ul li.off{height:70px; line-height: 70px; border-bottom: 1px solid #e5e5e5;font-size: 26px;color: #333333; padding: 0 20px; text-align: center;cursor: pointer;}
.area-box .tabcon ul li.on{height:70px; line-height: 70px; border-bottom: 1px solid #e5e5e5;font-size: 26px;color: #333333; padding: 0 20px;text-align: center;cursor: pointer; background: #f0fafc}
.area-box .tabcon ul li.none{height:70px; line-height: 70px; border-bottom: 1px solid #e5e5e5;font-size: 26px;color: #bbb; padding: 0 20px; text-align: center;cursor: not-allowed;pointer-events: none;}

.type-box{padding: 25px;background: #fff;}
.type-box p.name{font-size: 30px;height: 40px;line-height: 40px;color: #333;}
.type-box p.name span{color: #f21000;display: inline-block;padding-left: 20px}
.type-box ul{overflow: hidden;}
.type-box ul li.off{min-width: 80px;text-align: center; float: left;margin: 10px 23px 10px 0; height: 30px;line-height: 30px; padding: 10px 20px;background: #eeeeee;font-size: 22px;color: #000;border-radius: 10px;cursor: pointer;}
.type-box ul li.on{min-width: 80px;text-align: center; float: left;margin: 10px 23px 10px 0; height: 30px;line-height: 30px; padding: 10px 20px;background: #f21000;font-size: 22px;color: #fff;border-radius: 10px;cursor: pointer;}
.type-box ul li.none{min-width: 80px;text-align: center; float: left;margin: 10px 23px 10px 0; height: 30px;line-height: 30px; padding: 10px 20px;background: #eeeeee;font-size: 22px;color: #bbb;  border-radius: 10px;cursor: not-allowed;pointer-events: none;}
.type-box a.btn{width: 580px; display: inline-block; margin-top: 20px; border-radius: 6px; text-align: center; height: 70px;line-height: 70px;font-size: 30px;color: #18b4ed;border: 1px solid #18b4ed}
.type-box a.btn:hover{background: #18b4ed;color: #fff}


.pro-btn{position: fixed;bottom: -3px;z-index: 99}
.pro-btn ul li{overflow: hidden;height: 95px;line-height: 95px; border-bottom: 1px solid #e5e5e5;  font-size: 24px;}
.pro-btn ul li.m3 a{float: left;display: block;}
.pro-btn ul li.m3 a.t1{width: 360px; display: block;  background: #303132;text-align: left; padding-left: 20px; color: #e7e7e7;cursor: pointer;}
.pro-btn ul li.m3 a.t1:hover{background: #030406}
.pro-btn ul li.m3 a.t1 span{color: #ff4646;font-size: 28px}
.pro-btn ul li.m3 a.t2{width: 260px;  font-size: 28px; color: #ffffff;  display: block; background: #ff5a5a;text-align: center;cursor: pointer;}
.pro-btn ul li.m3 a.t2:hover{background: #FC3434}
.pro-btn ul li.m3 a.t3{width: 380px;  font-size: 28px; color: #ffffff; display: block; background: #303132;cursor: pointer;}
.pro-btn ul li.m3 a.t3:hover{background: #030406}
.pro-btn ul li.m3 a.t3 span{ display: block; padding-left: 50px; width: 270px;margin: 0 auto; background: url("../images/ico/zx_ico.png") no-repeat left center;}

