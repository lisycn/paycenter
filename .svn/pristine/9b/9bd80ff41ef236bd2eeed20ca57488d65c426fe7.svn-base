.pay-succ .text{padding-left: 90px; min-height: 90px;}
.pay-succ .succ{background: url("../images/ico/succ_ico.png") no-repeat left center;}
.pay-succ .err{background: url("../images/ico/error_ico.png") no-repeat left center;}
.pay-succ .text span.p1{display: block; font-size: 30px;color: #000;height: 50px;line-height: 50px;}
.pay-succ .text span.p2{display: block; font-size: 24px;color: #999;height: 40px;line-height: 40px;}
.pay-info{margin-left: 30px;border-top: 1px solid #e5e5e5;padding: 20px 0}
.pay-info .box{height: 30px;line-height: 30px;padding: 10px 0;}
.pay-info .box label{display: inline-block;width: 150px;font-size: 24px;color: #666;}
.pay-info .box span{display: inline-block;width: 430px;text-align: right;font-size: 24px;color: #666}
.pay-info .box span.num{color: #eb1f1f}
.pay-box .btn-box{height: 50px;line-height: 50px; background: #fff; padding: 20px 0;border-bottom: 1px solid #e5e5e5;border-top: 1px solid #e5e5e5;}
.pay-box .btn-box a.btn{display:inline-block; width: 319px; font-size: 24px;color: #18b4ed; float: left;  text-align: center;border-right: 1px solid #e5e5e5 }
.pay-box .btn-box a.btn:last-child{border-right: none}
.pay-box .pro-list .total{height: 60px; line-height: 60px; padding: 20px 0;margin-left: 30px;border-top: 1px solid #e5e5e5;padding-right: 30px}
.pay-box .pro-list .total p{font-size: 30px;color: #333;text-align: right;}
.pay-box .pro-list .total p span{color: #ff4848;display: inline-block;min-width: 120px}

/*订单*/
.order-box{}
.order-box .box{padding: 20px 30px;background: #fff;min-height: 50px;border-bottom: 1px solid #e5e5e5;border-top: 1px solid #e5e5e5;margin-bottom: 20px}
.order-box .box .name{font-size: 30px;color: #333;height: 50px;line-height: 50px}
.order-box .box .statu-box{height: 40px;line-height: 30px;overflow: hidden;}
.order-box .box .statu-box .statu{width: 460px;float: left;}
.order-box .box .statu-box .statu span.name{font-size: 24px;color: #666;display: inline-block;width: 120px}
.order-box .box .statu-box .statu span.tag{font-size: 24px;color: #666;display: inline-block;min-width: 80px;background: #17c887;color: #fff;text-align: center;padding: 0 5px;margin-right: 20px}
.order-box .box .statu-box .statu span.tag1{font-size: 24px;color: #666;display: inline-block;min-width: 80px;background: #ff9c00;color: #fff;text-align: center;padding: 0 5px;margin-right: 20px}
.order-box .box .statu-box .statu span.tag2{font-size: 24px;color: #666;display: inline-block;min-width: 80px;background: #ff5252;color: #fff;text-align: center;padding: 0 5px;margin-right: 20px}
.order-box .box .statu-box .statu span.time{font-size: 24px;color: #666;display: inline-block;min-width: 110px}
.order-box .box .statu-box  a.show{font-size: 24px;color: #0093e1;height: 40px;line-height: 40px;display: block; padding: 5px 0; float: left;}

/*个人中心*/
.center-bg{ height: 300px; background:url("../images/center/center_bg.png") no-repeat }
.user-box{width: 210px;height: 250px;margin: 0 auto}
.user-pic{width: 150px;margin: 0 auto;padding: 45px 0 10px 0 }
.user-tel p.tel{font-size: 32px;color: #ffffff;width: 210px;text-align: center;margin: 0 auto}
.user-login a.login{font-size: 28px;display: block;width: 100px;text-align: center;height: 50px;line-height: 50px;border: 1px solid #fff;color: #fff;border-radius: 8px;margin: 0 auto}
.user-info ul li{overflow: hidden;cursor: pointer;}
.user-info ul li a{color: #666666;display: block;}
.user-info ul li{height: 90px; line-height: 90px;  border-bottom: 1px solid #d5d5d5;padding: 0 20px 0 80px;font-size: 28px;}
.user-info ul li.u1{background: #ffffff url("../images/center/center_ico1.png") no-repeat center}
.user-info ul li.u2{background: #ffffff url("../images/center/center_ico2.png") no-repeat center}
.user-info ul li.u2 a {position:relative;} 
.user-info ul li.u2 a i.dot{display: block;width: 10px;height: 10px;background: url("../images/ico/dot.png") no-repeat center;position: absolute;top: 25px;left: 170px;}
.user-info ul li.u3{background: #ffffff url("../images/center/center_ico3.png") no-repeat center;}
.user-info ul li.u4{background: #ffffff url("../images/center/center_ico4.png") no-repeat center;border-top:1px solid #d5d5d5;margin-top: 20px; }
.user-info ul li.u5{background: #ffffff url("../images/center/center_ico5.png") no-repeat center;border-top:1px solid #d5d5d5;margin-top: -1px }
.user-info ul li.tel{background: #ffffff url("../images/center/tel_ico.png") no-repeat center;border-top:1px solid #d5d5d5;color: #ff5f5f;text-align: center; margin-top: 20px;}
.qbanner{width: 640px;height: 220px;background: url("../images/center/cenbanner.jpg") no-repeat center;position: relative;}
.qbanner .tel{position: absolute; top: 60px;right: 50px; text-align: right; height: 40px;font-size: 36px;color: #15b6eb}
.qbanner .call{position: absolute; top: 120px;right: 50px; width: 150px; text-align: center; height: 55px; line-height: 55px; border: 1px solid #15b6eb;border-radius: 8px; font-size: 24px;color: #15b6eb}

/*服务*/
.service-box {width:100%;position:relative;}
.service-box .tabbtn {height:90px; width: 640px;  background: #fff;}
.service-box .tabbtn li{float: left;width: 320px; padding: 15px 0 11px;border-bottom: 4px solid #fff;cursor: pointer;}
.service-box .tabbtn li a{font-size: 30px;color: #666;display: block;width: 260px; margin: 0 60px; text-align: center;height:60px;line-height: 60px; border-right: 1px solid #e5e5e5;}
.service-box .tabbtn li.current{border-bottom: 4px solid #18b4ed}
.service-box .tabbtn li.current a{color: #18b4ed}
.m-order ul.order li .zbox1{height: 60px; line-height: 60px;padding: 0 30px;}
.m-order ul.order li .zbox1 p.order{float:left;font-size: 22px;color: #000}
.m-order ul.order li .zbox1 p.time{float:right;color: #666666;font-size: 22px}

.m-order ul.order li .zbox2{padding: 30px 0;min-height: 50px; overflow: hidden; border-top: 1px solid #e5e5e5;margin-left: 30px;}
.m-order ul.order li .zbox2 .name{width: 480px;float: left;}
.m-order ul.order li .zbox2 .name p.p1{height: 35px; line-height: 35px;font-size: 24px;color: #666;}
.m-order ul.order li .zbox2 .name p.p1 span.tag{background: #17c887;color: #fff;padding: 0 5px;}
.m-order ul.order li .zbox2 .show{width: 110px;float: left;color: #0093e1;display: block;cursor: pointer;margin-top: 20px}

.show-info{padding: 30px;background: #ffffe7}
.show-info .list{padding: 5px 0}
.show-info .list label{font-size: 24px;color: #666;width: 120px;text-align: right; display: inline-block; font-size: 24px;color: #666;text-align: right;}
.show-info .list span{font-size: 24px;color: #666;width: 400px;}
.show-info .list span.tag{background: #17c887;color: #fff;padding: 0 5px}
.show-info .list span.tel a{color: #13b8f4;display: inline-block;width: 170px;padding-right: 110px;background: url("../images/ico/call_ico.png") no-repeat center right;}
.show-name{font-size: 28px;color: #333;height: 30px;line-height: 30px;padding: 30px;background: #fff;border-top: 1px solid #e5e5e5;border-bottom: 1px solid #e5e5e5}

/*登录*/
.login label{display: inline-block;width: 140px;text-align: right;float: left;font-size: 28px;color: #000000;margin-right: 10px}
.login .w-text input{height: 40px; line-height: 40px; padding: 0 10px; width: 260px;font-size: 28px;border: none;float: left;}
.login .list{position: relative;}
.login .list .getcode{color: #18b4ed;font-size: 24px;position: absolute;right: 30px;top: 20px;}
.login .list .piccode{width: 140px;height: 65px;display: block;position: absolute;right: 30px;top: 7px;}
.pay-box .sytime{font-size: 24px;color: #999999;text-align: right;padding: 30px}
.pay-box .sytime span{color: #f01a1a}

.pay-box .errbtn{width: 540px;text-align: center;height: 100px;line-height: 100px;font-size: 28px;color: #fff;background: #303132;border-radius: 10px;display: block;margin: 10px auto}